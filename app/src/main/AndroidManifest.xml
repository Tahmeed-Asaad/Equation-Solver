<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/abc"
        android:label="EQUATION SOLVER"
        android:roundIcon="@mipmap/abc"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".nonLinearActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".LinearActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".multipleVariableGauss"
            android:screenOrientation="portrait"/>
        <activity android:name=".quadratic"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>